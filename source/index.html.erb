  </div>
</section>

<section id="posts" class="posts">
  <div id="spinner" class="spinner">
    <div>
      <div class="spinner__dot1"></div>
      <div class="spinner__dot2"></div>
    </div>
  </div>
  <ul>
    <script id="handlebars-posts" type="text/x-handlebars-template">
      {{#each posts}}
      <li>
        <a href="{{url}}" target="_blank">
          <h3>{{title}}</h3>
          <div style="background-image: url({{image_url}})"></div>
        </a>
      </li>
      {{/each}}
    </script>
  </ul>
</section>
